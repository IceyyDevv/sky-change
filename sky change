function click()
	s = Instance.new("Sky")
	s.Name = "Sky"
	s.Parent = game.Lighting
	s.SkyboxBk = "http://www.roblox.com/asset/?id=ADDIMAGEHERE"
	s.SkyboxDn = "http://www.roblox.com/asset/?id=ADDIMAGEHERE"
	s.SkyboxFt = "http://www.roblox.com/asset/?id=ADDIMAGEHERE"
	s.SkyboxLf = "http://www.roblox.com/asset/?id=ADDIMAGEHERE"
	s.SkyboxRt = "http://www.roblox.com/asset/?id=ADDIMAGEHERE"
	s.SkyboxUp = "http://www.roblox.com/asset/?id=ADDIMAGEHERE"
end

script.Parent.MouseButton1Down:connect(click)
